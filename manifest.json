{
  "manifest_version": 2,
  "name": "Tiktok Videos Volume Changer",
  "description": "Change volume from Tiktok Videos.",
  "version": "1.2",
  "content_scripts": [
    {
      "matches": ["https://www.tiktok.com/*", "https://www.tiktok.com/*/*", "https://www.tiktok.com/*/*/*", "https://www.tiktok.com/*/*/*/*"],
      "js": ["index.js"]
    }
  ],
  "icons": {
    "16": "assets/icon16.png",
    "32": "assets/icon32.png",
    "128": "assets/icon128.png"
  }
}